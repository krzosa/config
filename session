session="python"

tmux new-session -s $session -d 
BUFF2=$(tmux new-window -t $session -P -F "#{pane_id}")
BUFF_COMPILATION=$(tmux split-window -t $session:1 -h -P -F "#{pane_id}")

tmux select-window -t $session:1
tmux select-pane -t $session:1.1

tmux bind b send-keys -t $BUFF_COMPILATION "py session.py" Enter \\\; swap-pane -t :1.2 -s $BUFF_COMPILATION \\\; select-pane -t :1.1 
tmux bind B swap-pane -t :1.2 -s $BUFF2
tmux a -t $session
